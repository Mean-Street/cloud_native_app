pipeline {
	agent any
		stages {
			stage('Build') {
				echo 'Building..'
				if (env.CHANGE_ID && env.BRANCH_NAME == "dev") {
					echo 'PR'
				} else if (env.BRANCH_NAME == "master") {
					echo 'Merge'
				} else {
					echo 'WTF'
				}
				sh './deployment/jenkins/scripts/build.py'
			}
			stage('Test') {
				steps {
					echo 'Testing..'
				}
			}
			stage('Deploy') {
				steps {
					echo 'Deploying....'
				}
			}
		}
}
