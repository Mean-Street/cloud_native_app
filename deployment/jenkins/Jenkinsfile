pipeline {
	agent any
		stages {
			stage('Test') {
				when { expression { allOf {
					env.CHANGE_ID;
					env.BRANCH_NAME == "dev"
				}}}
				steps {
					echo 'Pull Request'
				}
			}
			stage('Deploy') {
				when { expression { allOf {
					not { env.CHANGE_ID };
					env.BRANCH_NAME == "master"
				}}}
				steps {
					echo 'Push'
				}
			}
		}
}
