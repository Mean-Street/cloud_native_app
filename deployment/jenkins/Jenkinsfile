pipeline {
	agent any
		stages {
			stage('Debug') {
				when { expression { "dev" == "master" } }
			}
			steps {
				echo 'LOLILOL'
			}


			stage('Test') {
				when { allOf {
					branch 'dev'
					expression {env.CHANGE_ID}
				}}
				steps {
					sh 'printenv'
					echo 'Pull Request'
				}
			}


			stage('Deploy') {
				when { allOf {
					branch 'master'
					not { expression { env.CHANGE_ID } }
				}}
				steps {
					sh 'printenv'
					echo 'Push'
				}
			}
		}
}
