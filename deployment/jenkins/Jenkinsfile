pipeline {
	agent any
		stages {
			stage('Build') {
				steps {
					echo 'Building..'
					sh 'printenv'
					sh 'pwd'
					sh './deployment/jenkins/scripts/build.py'
				}
			}
			stage('Test') {
				steps {
					echo 'Testing..'
				}
			}
			stage('Deploy') {
				steps {
					echo 'Deploying....'
				}
			}
		}
}
